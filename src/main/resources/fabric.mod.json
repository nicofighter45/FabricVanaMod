  "name": "Fabric Vana Mod",
  "description": "Mod for minecraft vanadium server !",
  "authors": [
    "nicofighter45",
    "Spiirits_"
  ],
  "contact": {
    "homepage": "https://github.com/nicofighter45/FabricVanaMod",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/vana-mod/icon.png",

